{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@run-ide-helper",
            "@post-root-package-install",
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@run-ide-helper"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "coverage": "php -d pcov.enabled=1 ./vendor/bin/phpunit --coverage-html ./tests/code-coverage-report",
        "fixer": "./vendor/bin/php-cs-fixer fix --config=\".php-cs-fixer.php\" --using-cache=no ",
        "diff": "./vendor/bin/php-cs-fixer fix --config=\".php-cs-fixer.php\" --using-cache=no --diff --dry-run "
    }
}
